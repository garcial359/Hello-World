        <activity
            android:name="com.chf.helloworld.SendSMSActivity"
            android:label="@string/title_activity_send_sms"></activity>
        <activity
            android:name="com.chf.helloworld.SendEmailActivity"
            android:label="@string/title_activity_send_email"></activity>
        <activity
            android:name="com.chf.helloworld.PreferencesActivity"
            android:label="@string/title_activity_preferences"></activity>
        <activity
            android:name="com.chf.helloworld.ToDoActivity"
            android:label="@string/title_activity_to_do"></activity>
    </application>

</manifest>
