        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.chf.helloworld.HelloWorld"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.chf.helloworld.TextViewsAndImages"
            android:label="TextViewsAndImages"></activity>
        <activity
            android:name="com.chf.helloworld.Tiling"
            android:label="Tiling"></activity>
        <activity
            android:name="com.chf.helloworld.Toasty"
            android:label="Toasty"></activity>
        <activity
            android:name="com.chf.helloworld.RadioButtons"
            android:label="RadioButtons"></activity>
        <activity
            android:name="com.chf.helloworld.ListViewActivity"
            android:label="ListViewActivity"></activity>
        <activity
            android:name="com.chf.helloworld.ImageViewActivity"
            android:label="ImageViewActivity"></activity>
        <activity
            android:name="com.chf.helloworld.FavoriteButton"
            android:label="FavoriteButton"></activity>
        <activity
            android:name="com.chf.helloworld.CheckBoxes"
            android:label="CheckBoxes"></activity>
        <activity
            android:name="com.chf.helloworld.BookActivity"
            android:label="BookActivity"></activity>
        <activity
            android:name="com.chf.helloworld.GridViewActivity"
            android:label="GridViewActivity"></activity>
        <activity
            android:name="com.chf.helloworld.SICalculatorActivity"
            android:label="SICalculatorActivity"></activity>
        <activity
            android:name="com.chf.helloworld.HomeScreen"
            android:label="HomeScreen"></activity>
        <activity
            android:name="com.chf.helloworld.TrafficLights"
